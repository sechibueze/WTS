{
  "name": "wts",
  "version": "1.0.0",
  "description": "A public bus transport system api",
  "main": "src/app.js",
  "scripts": {
    "start": "set DEBUG=dev:* && nodemon --exec babel-node src/app.js",
    "preserve": "babel src --out-dir dist",
    "serve": "node dist/app.js",
    "test": "cross-env NODE_ENV=test nyc mocha --timeout 10000 src/test/index.spec.js",
    "test-with-coverage": "nyc --reporter=text --reporter=html mocha",
    "coverage": "babel-node ./node_modules/istanbul/lib/cli cover _mocha src/test/index.js",
    "report-coverage": "cat ./coverage/lcov.info | coveralls"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sechibueze/WTS.git"
  },
  "keywords": [
    "wayfarer",
    "transport",
    "api",
    "nodejs",
    "expressjs",
    "postgresql",
    "heroku"
  ],
  "author": "sechibueze",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/sechibueze/WTS/issues"
  },
  "homepage": "https://github.com/sechibueze/WTS#readme",
  "devDependencies": {
    "@babel/core": "^7.5.4",
    "@babel/node": "^7.5.0",
    "@babel/preset-env": "^7.5.4",
    "@babel/register": "^7.4.4",
    "babel-cli": "^6.26.0",
    "babel-plugin-istanbul": "^5.1.4",
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "coveralls": "^3.0.5",
    "cross-env": "^5.2.0",
    "eslint": "^5.16.0",
    "eslint-config-airbnb-base": "^13.2.0",
    "eslint-plugin-import": "^2.18.0",
    "mocha-lcov-reporter": "^1.3.0",
    "nodemon": "^1.19.1",
    "nyc": "^14.1.1"
  },
  "dependencies": {
    "express": "^4.17.1",
    "mocha": "^6.1.4",
    "pg": "^7.11.0"
  },
  "nyc": {
    "require": [
      "@babel/register"
    ],
    "reporter": [
      "lcov",
      "text"
    ],
    "sourceMap": false,
    "instrument": false
  },
  "engines": {
    "node": "10.13.0",
    "npm": "6.4.1"
  }
}